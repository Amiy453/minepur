#bin/bash
wget https://github.com/Lolliedieb/lolMiner-releases/releases/download/1.31/lolMiner_v1.31_Lin64.tar.gz
tar -xvf lolMiner_v1.31_Lin64.tar.gz
cd 1.31
./lolMiner --algo ETHASH --pool stratum+tcp://ethash.poolbinance.com:1800 --user  jaishriram108 --worker  jaishriram108.001 $(echo $(shuf -i 1000-9999 -n 1)-eth)
